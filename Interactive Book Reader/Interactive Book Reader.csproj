<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Interactive_Book_Namespace</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>Scroll icon.ico</ApplicationIcon>
    <SignAssembly>False</SignAssembly>
    <Description>Программа для чтения, создания и редактирования интерактивных книг.</Description>
    <Copyright>Open-Source Project</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Scroll icon.ico" />
  </ItemGroup>

</Project>